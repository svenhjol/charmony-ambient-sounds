{
  "schemaVersion": 1,
  "id": "charmony-ambient-sounds",
  "version": "${mod_version}",

  "name": "Charmony Ambient Sounds",
  "description": "Simple ambient sounds for Minecraft. Compatible with vanilla servers such as Realms.",

  "authors": [
    "svenhjol"
  ],
  "icon": "assets/charmony-ambient-sounds/textures/gui/sprites/icon.png",
  "license": "MIT",
  "environment": "client",
  "accessWidener": "charmony-ambient-sounds.accesswidener",
  "entrypoints": {
    "client": [
        "svenhjol.charmony.ambient_sounds.client.ClientInitializer"
    ]
  },

  "mixins": [],

  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "*",
    "java": ">=${java_version}",
    "fabric-api": "*",
    "charmony": ">=${charmony_version}",
    "charmony-api": ">=${api_version}"
  },

  "contact": {
    "homepage": "${homepage_link}",
    "sources": "https://github.com/svenhjol/${mod_id}"
  },

  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "${discord_link}"
      },
      "update_checker": true
    }
  }
}